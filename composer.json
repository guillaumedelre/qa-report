{
    "name": "guillaumedelre/qat",
    "type": "library",
    "description": "QA-Tools (alias QAT) helps you manipulate the qa metrics of your project.",
    "license": "MIT",
    "require": {
        "herrera-io/phar-update": "~1.0",
        "symfony/console": "^4.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8"
    },
    "autoload": {
        "psr-4": {
            "Agilicode\\QAT\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Agilicode\\QAT\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit --colors=always"
    }
}
