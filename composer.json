{
    "name": "guillaumedelre/qat",
    "type": "library",
    "description": "QA-Tools (alias QAT) helps you manipulate the qa metrics of your project.",
    "license": "MIT",
    "require": {
        "herrera-io/phar-update": "~1.0",
        "symfony/flex": "^1.2",
        "symfony/config": "^4.2",
        "symfony/yaml": "^4.2",
        "symfony/options-resolver": "^4.2",
        "symfony/dependency-injection": "^4.2",
        "symfony/dotenv": "^4.2",
        "symfony/framework-bundle": "^4.2",
        "symfony/console": "^4.2",
        "doctrine/annotations": "^1.6",
        "symfony/validator": "^4.2",
        "doctrine/inflector": "^1.3",
        "symfony/monolog-bundle": "^3.3",
        "symfony/serializer-pack": "^1.0",
        "symfony/twig-bundle": "^4.2",
        "symfony/event-dispatcher": "^4.2",
        "symfony/workflow": "^4.2",
        "symfony/webpack-encore-bundle": "^1.5",
        "twig/extensions": "^1.5",
        "codacy/coverage": "^1.4",
        "guillaumedelre/junit-log-parser": "dev-master",
        "khill/php-duration": "^1.0",
        "symfony/translation": "^4.2",
        "swaggest/json-schema": "^0.12.9"
    },
    "require-dev": {
        "phpunit/phpunit": "^8",
        "symfony/var-dumper": "^4.2"
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/guillaumedelre/JUnitLogParser.git"
        }
    ],
    "conflict": {
        "symfony/symfony": "*"
    },
    "autoload": {
        "psr-4": {
            "Agilicode\\QAT\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Agilicode\\QAT\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit --colors=always"
    }
}
