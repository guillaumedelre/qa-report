{% extends 'base.html.twig' %}

{% block content %}
    <main role="main" class="container-fluid">

        <div class="d-flex align-items-center p-3 my-3 text-dark-50 bg-white rounded shadow-sm">
            <img class="mr-3" src="data:image/png;base64," alt="" width="48" height="48">
            <div class="lh-100">
                <h6 class="mb-0 text-dark lh-100">{{ page_title|capitalize|e }}</h6>
                <small>FranceMediasMonde</small>
            </div>
        </div>

        <div class="row my-3">
            {% for project in projects %}
                <div class="col-2 my-3">
                    <img class="img-fluid" src="{{ assets_path }}/{{ project.name }}.svg">
                </div>
            {% endfor %}
        </div>
    </main>
{% endblock %}
