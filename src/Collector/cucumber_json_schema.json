{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "title": "Behat cucumber_json report",
  "items": {
    "$id": "#/items",
    "type": "object",
    "title": "The Items Schema",
    "required": [
      "uri",
      "id",
      "tags",
      "keyword",
      "name",
      "line",
      "description",
      "elements"
    ],
    "properties": {
      "uri": {
        "$id": "#/items/properties/uri",
        "type": "string",
        "title": "The Uri Schema",
        "default": "",
        "examples": [
          "features/login/login.feature"
        ]
      },
      "id": {
        "$id": "#/items/properties/id",
        "type": "string",
        "title": "The Id Schema",
        "default": "",
        "examples": [
          "get-a-jwt-token-through-a-basic-auth"
        ]
      },
      "tags": {
        "$id": "#/items/properties/tags",
        "type": "array",
        "title": "The Tags Schema",
        "items": {
          "$id": "#/items/properties/tags/items",
          "type": "object",
          "title": "The Items Schema",
          "required": [
            "name"
          ],
          "properties": {
            "name": {
              "$id": "#/items/properties/tags/items/properties/name",
              "type": "string",
              "title": "The Name Schema",
              "default": "",
              "examples": [
                "@token"
              ]
            }
          }
        }
      },
      "keyword": {
        "$id": "#/items/properties/keyword",
        "type": "string",
        "title": "The Keyword Schema",
        "default": "",
        "examples": [
          "Feature"
        ]
      },
      "name": {
        "$id": "#/items/properties/name",
        "type": "string",
        "title": "The Name Schema",
        "default": "",
        "examples": [
          "Get a jwt token through a basic auth"
        ]
      },
      "line": {
        "$id": "#/items/properties/line",
        "type": "integer",
        "title": "The Line Schema",
        "default": 0,
        "examples": [
          0
        ]
      },
      "description": {
        "$id": "#/items/properties/description",
        "type": ["null","string"],
        "title": "The Description Schema",
        "default": null,
        "examples": [
          null
        ]
      },
      "elements": {
        "$id": "#/items/properties/elements",
        "type": "array",
        "title": "The Elements Schema",
        "items": {
          "$id": "#/items/properties/elements/items",
          "type": "object",
          "title": "The Items Schema",
          "required": [
            "id",
            "tags",
            "keyword",
            "name",
            "line",
            "description",
            "type",
            "steps",
            "examples"
          ],
          "properties": {
            "id": {
              "$id": "#/items/properties/elements/items/properties/id",
              "type": "string",
              "title": "The Id Schema",
              "default": "",
              "examples": [
                ";get-a-jwt-token-through-a-basic-auth"
              ]
            },
            "tags": {
              "$id": "#/items/properties/elements/items/properties/tags",
              "type": "array",
              "title": "The Tags Schema",
              "items": {
                "$id": "#/items/properties/elements/items/properties/tags/items",
                "type": "object",
                "title": "The Items Schema",
                "required": [
                  "name"
                ],
                "properties": {
                  "name": {
                    "$id": "#/items/properties/elements/items/properties/tags/items/properties/name",
                    "type": "string",
                    "title": "The Name Schema",
                    "default": "",
                    "examples": [
                      "@clearDatabase"
                    ]
                  }
                }
              }
            },
            "keyword": {
              "$id": "#/items/properties/elements/items/properties/keyword",
              "type": "string",
              "title": "The Keyword Schema",
              "default": "",
              "examples": [
                "Scenario"
              ]
            },
            "name": {
              "$id": "#/items/properties/elements/items/properties/name",
              "type": ["null","string"],
              "title": "The Name Schema",
              "default": null,
              "examples": [
                null
              ]
            },
            "line": {
              "$id": "#/items/properties/elements/items/properties/line",
              "type": "integer",
              "title": "The Line Schema",
              "default": 0,
              "examples": [
                5
              ]
            },
            "description": {
              "$id": "#/items/properties/elements/items/properties/description",
              "type": "string",
              "title": "The Description Schema",
              "default": "",
              "examples": [
                ""
              ]
            },
            "type": {
              "$id": "#/items/properties/elements/items/properties/type",
              "type": "string",
              "title": "The Type Schema",
              "default": "",
              "examples": [
                "scenario"
              ]
            },
            "steps": {
              "$id": "#/items/properties/elements/items/properties/steps",
              "type": "array",
              "title": "The Steps Schema",
              "items": {
                "$id": "#/items/properties/elements/items/properties/steps/items",
                "type": "object",
                "title": "The Items Schema",
                "required": [
                  "keyword",
                  "name",
                  "line",
                  "match",
                  "result",
                  "arguments"
                ],
                "properties": {
                  "keyword": {
                    "$id": "#/items/properties/elements/items/properties/steps/items/properties/keyword",
                    "type": "string",
                    "title": "The Keyword Schema",
                    "default": "",
                    "examples": [
                      "Given"
                    ]
                  },
                  "name": {
                    "$id": "#/items/properties/elements/items/properties/steps/items/properties/name",
                    "type": "string",
                    "title": "The Name Schema",
                    "default": "",
                    "examples": [
                      "the following fixtures are loaded:"
                    ]
                  },
                  "line": {
                    "$id": "#/items/properties/elements/items/properties/steps/items/properties/line",
                    "type": "integer",
                    "title": "The Line Schema",
                    "default": 0,
                    "examples": [
                      6
                    ]
                  },
                  "match": {
                    "$id": "#/items/properties/elements/items/properties/steps/items/properties/match",
                    "type": "object",
                    "title": "The Match Schema",
                    "required": [
                      "location"
                    ],
                    "properties": {
                      "location": {
                        "$id": "#/items/properties/elements/items/properties/steps/items/properties/match/properties/location",
                        "type": "string",
                        "title": "The Location Schema",
                        "default": "",
                        "examples": [
                          ""
                        ]
                      }
                    }
                  },
                  "result": {
                    "$id": "#/items/properties/elements/items/properties/steps/items/properties/result",
                    "type": "object",
                    "title": "The Result Schema",
                    "required": [
                      "status",
                      "error_message",
                      "duration"
                    ],
                    "properties": {
                      "status": {
                        "$id": "#/items/properties/elements/items/properties/steps/items/properties/result/properties/status",
                        "type": "string",
                        "title": "The Status Schema",
                        "default": "",
                        "examples": [
                          "passed"
                        ]
                      },
                      "error_message": {
                        "$id": "#/items/properties/elements/items/properties/steps/items/properties/result/properties/error_message",
                        "type": ["null","string"],
                        "title": "The Error_message Schema",
                        "default": null,
                        "examples": [
                          null
                        ]
                      },
                      "duration": {
                        "$id": "#/items/properties/elements/items/properties/steps/items/properties/result/properties/duration",
                        "type": "number",
                        "title": "The Duration Schema",
                        "default": 0,
                        "examples": [
                          2027000000.0000002
                        ]
                      }
                    }
                  },
                  "arguments": {
                    "$id": "#/items/properties/elements/items/properties/steps/items/properties/arguments",
                    "type": "array",
                    "title": "The Arguments Schema",
                    "items": {
                      "$id": "#/items/properties/elements/items/properties/steps/items/properties/arguments/items",
                      "type": "string",
                      "title": "The Items Schema",
                      "default": "",
                      "examples": [
                        "Fmm\\Mezzo\\AuthenticationBundle\\Entity\\User:\n  admin:\n    id: \"c9ef8ade-9aa5-11e6-baaf-080027d5f839\"\n    username: \"admin\"\n    blacklisted: false\n    refreshToken: \"ba4e5484-8fa6-11e6-8a49-080027d5f839\"\n    createdAt: \"2018-09-17 00:00:00\"\n    updatedAt: \"2018-09-17 00:00:00\"\n  blacklisted:\n    id: \"1dac1820-ba7c-11e8-96f8-529269fb1459\"\n    username: \"blacklisted_user\"\n    blacklisted: true\n    refreshToken: \"8d4ef1b8-6d38-4145-a9e0-917af45a4ee5\"\n    createdAt: \"2018-09-17 00:00:00\"\n    updatedAt: \"2018-09-17 00:00:00\"\n\nFmm\\Mezzo\\AuthenticationBundle\\Entity\\Application:\n  application:\n    id: \"9a85bca0-c61d-11e8-a355-529269fb1459\"\n    label: \"BO\"\n\nFmm\\Mezzo\\AuthenticationBundle\\Entity\\Role:\n  role:\n    id: \"c478f1f8-c61d-11e8-a355-529269fb1459\"\n    label: \"ROLE_ADMIN\"\n\nFmm\\Mezzo\\AuthenticationBundle\\Entity\\UserApplicationRole:\n  userApplicationRole:\n    id: \"56eeb3ea-c729-11e8-a8d5-f2801f1b9fd1\"\n    user: \"@admin\"\n    application: \"@application\"\n    roles: [\"@role\"]"
                      ]
                    }
                  }
                }
              }
            },
            "examples": {
              "$id": "#/items/properties/elements/items/properties/examples",
              "type": "array",
              "title": "The Examples Schema"
            }
          }
        }
      }
    }
  }
}
